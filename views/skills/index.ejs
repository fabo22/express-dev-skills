<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skills</title>
</head>
<body>
    <h1>Skills</h1>
    <a href="/skills/new">Add New Skill</a>
    <ul>
      <% allSkills.forEach(function(skill, id) { %>
        <li>
          <a href="/skills/<%=id%>"><%= skill.title %></a>
        </li>
      <% }); %>
    </ul>
</body>
</html>